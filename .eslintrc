{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "__DEV__": true
  },
  "env": {
    "browser": true,
    "jest": true,
    "jquery": true,
  },
  "rules": {
    "no-confusing-arrow": 0,
    "react/jsx-quotes": 0,
    "jsx-quotes": 0,
    "eol-last": 0,
    "max-len": 0,
    "no-console": 0,
    "object-shorthand": 0,
    "react/jsx-no-bind": 0,
    "prefer-template": 0,
    "react/prop-types": 0,
  }
}
